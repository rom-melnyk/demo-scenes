{
  "name": "demo-scenes",
  "version": "1.0.0",
  "description": "Demo scenes (HTML/CSS, Canvas, Typescript)",
  "main": "./src/ts/index.ts",
  "scripts": {
    "dev": "rm -fr dist > /dev/null && parcel serve -p 9000 --public-url . ./src/index.html",
    "demo": "rm -fr dist > /dev/null && parcel serve -p 9000 --no-cache --no-source-maps --open ./src/index.html",
    "prod": "rm -fr dist > /dev/null && parcel build --public-url . --no-cache --no-source-maps ./src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rom-melnyk/demo-scenes.git"
  },
  "keywords": [
    "ts",
    "typescript",
    "scss",
    "parcel",
    "canvas",
    "paint",
    "draw",
    "boomerang",
    "girona",
    "spain"
  ],
  "author": "Roman Melnyk <https://melnyk.site>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rom-melnyk/demo-scenes/issues"
  },
  "homepage": "https://github.com/rom-melnyk/demo-scenes#readme",
  "devDependencies": {},
  "dependencies": {
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.24.4",
    "typescript": "^3.7.4"
  }
}
